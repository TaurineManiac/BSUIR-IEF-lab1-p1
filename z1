#include <stdio.h>
#define _USE_MATH_DEFINES 
#include <math.h> 
void main()
{
	double r, h, s,v,s1;
    	scanf_s("%lf", &h);
	    scanf_s("%lf", &r);
	        s = M_PI * r * h * 2;
			s1 = (M_PI * r * h * 2)+(2*M_PI*r*r);
	        v = M_PI * r * r * h;
	printf("%.2f\n", s);
	printf("%.2f\n", s1);
	printf("%.2f", v);
}
